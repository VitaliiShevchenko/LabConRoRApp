<td class="cell read">
  <input type="checkbox"
         id=<%= rec.id %>
         name='checkBox'
         <%= @brand_receipts.last.id == rec.id ? "checked" : ""%>
    />
</td>
<td class="cell read"><%= rec.id %></td>
<td class="cell read"><%= rec.receipt_num %></td>
<td class="cell read"><%= number_with_precision rec.density, precision: 3 %></td>
<td class="cell read"><%= rec.hardnessShA %></td>
<td class="cell read"><%= rec.hardnessShD %></td>
<td class="cell read"><%= number_to_currency(rec.calculated_cost) %></td>
<td class="cell read"><%= rec.created_at %></td>
<td class="cell read"><%= time_ago_in_words rec.updated_at %></td>